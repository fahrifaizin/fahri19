<div class="container">
  <div class="row">
      <div class="class text-center">
          <h1 class="display-4">Daftar Contact</h1>
          <table class="table">
              <thead>
              <tr>
                  <th scope="col">Nama</th>
                  <th scope="col">Email</th>
              </tr>
              </thead>
              <tbody>
                  <% contacts.forEach((contact, i) => { i++ %>
                      <tr>
                          <th scope="row"><%= i++ %></th>
                          <td><%= contact.nama %></td>
                          <td><%= contact.email %></td>
                      </tr>
                  <% }) %>
              </tbody>
          </table>
      </div>
  </div>
</div>
